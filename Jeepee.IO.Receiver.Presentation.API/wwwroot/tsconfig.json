{
    "compilerOptions": {
      "target": "es5",
      "jsx": "react",
      "module": "es2015",
      "lib": ["es6", "dom"],
      "moduleResolution": "node",
      "esModuleInterop": true,
      "sourceMap": true,
      "baseUrl": ".",
      "paths": {
        "@src/*": ["src/*"],
        "@config/*": ["src/config/*"]
      }
    },
    "exclude": ["node_modules"]
}